<%= form_for [@project, @part] do |f| %>
  <%= f.label :part_number %>
  <%= f.text_field :part_number %><br />

  <%= f.label :name %>
  <%= f.text_field :name %><br />

  <%= f.label :description %>
  <%= f.text_area :description %><br />

  <%= f.label :datasheet_url, "Datasheet URL" %>
  <%= f.text_field :datasheet_url %><br />

  <%= f.submit "Add" %>
<% end %>
